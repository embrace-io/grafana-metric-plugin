{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Embrace",
  "id": "embraceio-metric-app",
  "info": {
    "keywords": ["android","ios","mobile","anr"],
    "description": "Mobile observability and data platform that empowers mobile teams to deliver optimal user experiences",
    "author": {
      "name": "Embrace",
      "url": "https://embrace.io/"
    },
    "logos": {
      "small": "img/logo.png",
      "large": "img/logo.png"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://embrace.io/"
      },
      {
        "name": "Documentation",
        "url": "https://embrace.io/docs/"
      }
    ],
    "screenshots": [
      {"name": "sessions", "path": "img/sessions.png"},
      {"name": "crashes_users", "path": "img/crashes_users.png"}
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "dashboard",
      "name": "Overview Dashboard",
      "path": "dashboards/overview.json",
      "addToNav": true,
      "defaultNav": true,
      "uid": "0-0000000"
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=10.4.0",
    "plugins": []
  }
}
