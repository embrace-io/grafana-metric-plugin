{
  "name": "embrace",
  "version": "1.3.1",
  "description": "Embrace",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "server": "podman-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin"
  },
  "author": "Embrace",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@grafana/e2e": "10.4.12",
    "@grafana/e2e-selectors": "12.3.0",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@swc/core": "^1.2.144",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.188",
    "@types/node": "^24.10.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^8.48.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^44.2.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^13.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^3.3.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.94.2",
    "sass-loader": "16.0.6",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.9.3",
    "webpack": "^5.69.1",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "9.2.5",
    "@grafana/runtime": "9.2.5",
    "@grafana/ui": "9.2.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^6.23.1"
  }
}
